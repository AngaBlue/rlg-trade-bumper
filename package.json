{
	"name": "rlg-trade-bumper",
	"version": "1.0.3",
	"description": "A web browser extension starter - based on Typescript, React, Redux, Styled-Component, Webpack, and more. Runs on Chrome and Firefox.",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack --mode development",
		"prod": "cross-env NODE_ENV=production webpack --mode production",
		"watch": "cross-env NODE_ENV=watch webpack --mode development",
		"clean": "rm -rf ./dist",
		"clean:all": "rm -rf ./dist && rm -rf ./coverage && rm -rf ./node_modules",
		"lint": "tslint -c tslint.json --fix 'src/**/*.ts*'"
	},
	"license": "MIT",
	"dependencies": {
		"@chakra-ui/core": "^0.8.0",
		"@emotion/core": "^10.0.35",
		"@emotion/styled": "^10.0.27",
		"css-loader": "^5.2.4",
		"dayjs": "^1.9.1",
		"emotion-theming": "^10.0.27",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"style-loader": "^2.0.0",
		"webextension-polyfill-ts": "^0.25.0"
	},
	"devDependencies": {
		"@types/chrome": "^0.0.135",
		"@types/node": "^14.14.41",
		"@types/react": "17.0.4",
		"@types/react-dom": "^17.0.3",
		"@types/react-test-renderer": "^17.0.1",
		"@types/webpack": "^5.28.0",
		"awesome-typescript-loader": "^5.2.1",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"copy-webpack-plugin": "^8.1.1",
		"cross-env": "^7.0.2",
		"html-webpack-plugin": "^5.3.1",
		"react-test-renderer": "^17.0.2",
		"tslint": "^6.0.0",
		"typescript": "^4.2.4",
		"webpack": "^5.35.1",
		"webpack-cli": "^4.6.0",
		"webpack-extension-reloader": "^1.1.4"
	},
	"resolutions": {
		"**/@types/react": "16.9.34"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ymdevs/Web-Extension-Starter.git"
	}
}
