{
	"name": "rlg-trade-bumper",
	"version": "1.0.3",
	"description": "A web browser extension starter - based on Typescript, React, Redux, Styled-Component, Webpack, and more. Runs on Chrome and Firefox.",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack --mode development",
		"prod": "cross-env NODE_ENV=production webpack --mode production",
		"watch": "cross-env NODE_ENV=watch webpack --mode development",
		"clean": "rm -rf ./dist",
		"clean:all": "rm -rf ./dist && rm -rf ./coverage && rm -rf ./node_modules",
		"lint": "tslint -c tslint.json --fix 'src/**/*.ts*'"
	},
	"license": "MIT",
	"dependencies": {
		"@chakra-ui/icons": "^1.0.14",
		"@chakra-ui/react": "^1.6.5",
		"@emotion/is-prop-valid": "^1.1.0",
		"@emotion/react": "^11.4.0",
		"@emotion/styled": "^11.3.0",
		"css-loader": "^6.2.0",
		"dayjs": "^1.9.1",
		"framer-motion": "^4.1.17",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"style-loader": "^3.2.1",
		"webextension-polyfill-ts": "^0.26.0"
	},
	"devDependencies": {
		"@types/chrome": "^0.0.152",
		"@types/node": "^16.4.10",
		"@types/react": "17.0.15",
		"@types/react-dom": "^17.0.3",
		"@types/react-test-renderer": "^17.0.1",
		"@types/webpack": "^5.28.0",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"copy-webpack-plugin": "^9.0.1",
		"cross-env": "^7.0.2",
		"html-webpack-plugin": "^5.3.1",
		"react-test-renderer": "^17.0.2",
		"ts-loader": "^9.2.4",
		"tslint": "^6.0.0",
		"typescript": "^4.2.4",
		"webpack": "^5.35.1",
		"webpack-cli": "^4.6.0",
		"webpack-extension-reloader": "^1.1.4"
	},
	"resolutions": {
		"**/@types/react": "16.9.34"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ymdevs/Web-Extension-Starter.git"
	}
